{
  "name": "@cloudpss/peers",
  "version": "0.1.0",
  "description": "CloudPSS p2p framework",
  "repository": "https://github.com/CloudPSS/peers",
  "author": "CloudPSS",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "browser": {
    "./dist/esm/polyfill": "./dist/esm/polyfill_browser.js",
    "./dist/cjs/polyfill": "./dist/cjs/polyfill_browser.js"
  },
  "scripts": {
    "start": "yarn clean && tsc --watch",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc --outDir dist/esm --module ESNext --target ESNext",
    "prepublishOnly": "yarn build",
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.ts .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@shelacek/ubjson": "^1.1.0",
    "@types/simple-peer": "^9.11.1",
    "core-js": "^3.16.0",
    "process": "^0.11.10",
    "rxjs": "^7.3.0",
    "simple-peer": "^9.11.0",
    "socket.io-client": "^4.1.3",
    "tslib": "^2.3.0",
    "type-fest": "^1.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@types/core-js": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "wrtc": "^0.4.7"
  }
}
