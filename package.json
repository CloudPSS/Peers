{
  "name": "@cloudpss/peers",
  "version": "0.0.6",
  "description": "CloudPSS p2p framework",
  "repository": "https://github.com/CloudPSS/peers",
  "author": "CloudPSS",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "start": "yarn clean && tsc --outDir dist/cjs --module commonjs --watch",
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --outDir dist/esm",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs --target ES5",
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.ts .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@shelacek/ubjson": "^1.1.0",
    "rxjs": "^7.0.1",
    "simple-peer": "^9.11.0",
    "socket.io-client": "^4.1.2",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@types/simple-peer": "^9.6.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^34.7.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "type-fest": "^1.1.1",
    "typescript": "^4.2.4",
    "wrtc": "^0.4.7"
  }
}
